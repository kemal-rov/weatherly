{
  "name": "weatherly",
  "version": "1.0.0",
  "description": "Geo & weather data integration service for real-time environmental insights.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npx jest",
    "test:verbose": "npx jest --verbose",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "test:ci": "npx jest --ci --reporters=default --reporters=jest-junit",
    "test:clear-cache": "npx jest --clearCache",
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "tsc && nodemon --watch \"src/**/*.ts\" --exec \"node\" dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kemal-rov/weatherly.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kemal-rov/weatherly/issues"
  },
  "homepage": "https://github.com/kemal-rov/weatherly#readme",
  "dependencies": {
    "axios": "^1.7.7",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "geo-tz": "^8.1.2",
    "http": "^0.0.1-security"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "express": "^4.21.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
